CREATE TABLE LiftingSet (
    id TEXT NOT NULL PRIMARY KEY,
    variationId TEXT NOT NULL,
    weight REAL,
    unit TEXT,
    reps INTEGER,
    tempoDown INTEGER,
    tempoHold INTEGER,
    tempoUp INTEGER
);

getAllByVariation:
SELECT * FROM LiftingSet WHERE variationId = ?;

get:
SELECT * FROM LiftingSet WHERE id = ?;

save:
INSERT OR REPLACE
INTO LiftingSet(id, variationId, weight, unit, reps, tempoDown, tempoHold, tempoUp)
VALUES(?, ?, ?, ?, ?, ?, ?, ?);
