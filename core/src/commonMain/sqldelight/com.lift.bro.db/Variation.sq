CREATE TABLE Variation (
    id TEXT NOT NULL PRIMARY KEY,
    liftId TEXT NOT NULL,
    name TEXT,
    pbWeight REAL,
    pbUnit TEXT
);

getAll:
SELECT * FROM Variation WHERE liftId = ?;

get:
SELECT * FROM Variation WHERE id = ?;

save:
INSERT OR REPLACE
INTO Variation(id, liftId, name, pbWeight, pbUnit)
VALUES(?, ?, ?, ?, ?);